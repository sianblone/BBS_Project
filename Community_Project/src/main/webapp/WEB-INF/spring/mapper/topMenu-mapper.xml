<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sif.community.dao.TopMenuDao">
	
	<select id="getTopMenuList" resultType="boardInfoVO">
		SELECT bi_id, bi_name
		FROM tbl_board_info
		WHERE bi_enabled = 1
		ORDER BY bi_id
	</select>
	
</mapper>